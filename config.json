{
  "something_else" : {

  },
  "logging": {
    "handlers": {
      "default": {
        "formatter": "standard",
        "class": "logging.StreamHandler",
        "level": "DEBUG"
      }
    },
    "loggers": {
      "loris": {
        "level": "DEBUG",
        "propagate": true,
        "handlers": ["default"]
      }
    },
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "standard": {
        "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
      }
    }
  }
}
